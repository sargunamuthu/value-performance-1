import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr =new int[n]; 
        for (int i = 0; i < n; i++)
    {
            arr[i] = sc.nextInt();
            }
            int positiveCount = 0;
            int negativeCount = 0;
    
            for(int num: arr) { 
       if(num > 0)
       {
       positiveCount++;    
       }
                else if (num < 0) {
       negativeCount++;
       }
       }
       System.out.printf("Count of Positive Integer is %.2f\n", (float) positiveCount);
       System.out.printf("Count of Negative Integer is %.2f\n",
       (float) negativeCount);
        sc.close();
    }
} 
