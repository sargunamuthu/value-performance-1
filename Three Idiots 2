import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        int X1=sc.nextInt();
        int Y1=sc.nextInt();
        int X2=sc.nextInt()
        double X=(X1+X2)/2.0;
        int Y2=sc.nextInt();;
        double Y=(Y1+Y2)/2.0;
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",X,Y);
    }
}
